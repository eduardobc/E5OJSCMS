/* start e5ojs form */
.e5ojs-login-wrapper .card-contentÂ {
    position: relative;
}
.e5ojs-login-wrapper .card-content .card-title {
    text-align: center;
}
.e5ojs-login-wrapper .card-title {
    text-shadow: 0px 0px 3px rgba(0,0,0,0.6);
}

.e5ojs-login-wrapper .btn-floating {
    position: absolute;
    margin: auto;
    right: 0;
    left: 0;
    bottom: -30px;
    transition: 0.5s;
    box-shadow: 0px 0px 4px 2px rgba(0,0,0,0.2);
}
.e5ojs-login-wrapper .btn-floating:hover {
    transform: rotate(360deg);
}
.e5ojs-login-wrapper .login-error-message {
    font-size: 20px;
    line-height: 20px;
    font-weight: 600;
    text-align: center;
    color: #0288d1;
    margin-bottom: 20px;
}
.e5ojs-login-wrapper input {
    display: block;
    width: 100%;
    color: #a8a8a8;
}
.e5ojs-login-wrapper button {
    border: none;
    color: #FFF !important;
}
.e5ojs-login-wrapper .e5ojs-form-login {
    padding-bottom: 15px;
}
.e5ojs-login-wrapper .vertical-center {
    position: fixed;
    top: 0;
    bottom: 0;
    display: block;
    margin: auto;
    max-width: 500px;
    width: 100%;
    right: 0;
    left: 0;
    max-height: 480px;
}

