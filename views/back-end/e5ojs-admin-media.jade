extends e5ojs-base-layout-admin
block e5ojs_block_local_css_script
    link(rel='stylesheet', href=e5ojs_global_data.e5ojs_base_url+'/back-end/css/e5ojs-admin-settings.css')
    script(type='text/javascript', src=e5ojs_global_data.e5ojs_base_url+'/back-end/js/e5ojs-settings-admin.js')
block e5ojs_block_content
            div(class="row")
                div(class="col-xs-12")
                    div(class="card")
                      div(class="card-content")
                        span(class="card-title")='IMAGES'
                        div(class="media-content")
                            - for( var i=0; i<=30; i++ ) {
                                div(class="media-item")
                                    img( class="img-responsive" src="https://placeholdit.imgix.net/~text?txtsize=28&bg=066DA9&txtclr=FFFFFF&txt=E5OJS&w=150&h=150&txttrack=0")
                            - }
                         
