            link(rel='stylesheet', href=host_url+'/front-end/css/e5ojs-global.css')
            script(type='text/javascript', src=host_url+'/front-end/js/e5ojs-global.js')
        block e5ojs_block_local_css_script
    body(class="body")
        div(class="e5o-all-wrapper")
            block e5ojs_block_content
        div(class="e5o-footer-wrapper")
